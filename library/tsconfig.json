{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": false,
    "removeComments": false,
    "outDir": "./tmp",
    "sourceMap": false,
    "jsx": "react",
    "module": "commonjs",
    "declaration": true,
    "newLine": "LF",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowSyntheticDefaultImports": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "resolution": "typings",
  "moduleResolution": "node",
  "include": [
    "src"
  ],
  "atom": {
    "rewriteTsconfig": false
  }
}
