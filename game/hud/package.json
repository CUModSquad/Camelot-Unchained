{
  "private": true,
  "name": "cu-ui-hud",
  "version": "0.1.0",
  "description": "Camelot Unchained UI - hud",
  "license": "MPL-2.0",
  "main": "dist/hud.js",
  "author": "City State Entertainment, LLC <support@citystateentertainment.com>",
  "contributors": [
    "Ortu"
  ],
  "keywords": [
    "Camelot",
    "Unchained",
    "cu",
    "cu-ui",
    "crafting"
  ],
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/csegames/cu-ui.git"
  },
  "watch": {
    "build": {
      "patterns": "src",
      "extensions": "ts,tsx,json"
    },
    "build:sass": {
      "patterns": "src",
      "extensions": "scss"
    },
    "copy:dist": {
      "patterns": "src",
      "extensions": "html,jpg,png,webm,ogg,svg,css,ico,js,json"
    },
    "browserify:lib": {
      "patterns": "../../library/lib",
      "extensions": "ts,js,css,scss"
    }
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "rootDir": "src"
  },
  "scripts": {
    "start": "nps",
    "postinstall": "rimraf typings && typings install && nps gql"
  },
